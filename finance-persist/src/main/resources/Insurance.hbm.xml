<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.dragon.drive.domain">

	<class name="Insurance" table="insurance">

		<id name="id" column="insuranceId">
			<generator class="native" />
		</id>

		<property name="name" />
		<property name="insuranceEndtime" />
		<property name="insuranceStarttime" />
		<property name="protector" />
		<property name="accidentTimes" />
		<property name="information" />
		<property name="cost" />
		<property name="status" />

		<many-to-one name="car" column="carId" unique="true"
			not-null="true"></many-to-one>
	</class>

</hibernate-mapping>
